{% if is_exist == true %}
  <div class="intro-farm">
    <div class="farm-name">
      <h3> WELCOME TO {{data.farm.name}}!</h3>
    </div>
    <div class="farm-image">
      <img src={{data.farm.farm_images}} alt="Farm Image">
    </div>
    <div class="farm-details">
      <p class="farm-description">
        {{data.farm.description}}
      </p>
    </div>
  </div>
{% endif %}
